{
  "manifest_version": 3,
  "name": "name",
  "version": "1.0",
  "description": "desc",
  "permissions": ["activeTab", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "index.html"
  },
  "icons": {
    "16": "",
    "48": "",
    "128": ""
  }
}
